#include<stdio.h>

int eh_primo();

int main (){
    
    int primo = 2;
    
    printf("Digite um inteiro positivo:\n");
    scanf("%d",&primo);
    
    eh_primo(primo);
    
}

int eh_primo(int p){
    
    int cont = 0;
    
    for(int i = 1; i <= p; i++){
        
        if((p%i) == 0) cont++;
        
    }
    
    
    if(cont == 2){
        
        printf("É primo") ;
        
    }else printf("Não é primo") ;
    
}
