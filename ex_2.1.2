#include <stdio.h>

void main(){
    
    int primeiro = 0, segundo = 0;
    
    printf("Digite o primeiro valor: \n");
    scanf("%d",&primeiro);
    printf("Digite o segundo valor: \n");
    scanf("%d",&segundo);
    
    if (primeiro > segundo){
        printf("O maior valor é: %d",primeiro);
    }
    else {
        printf("O maior valor é: %d",segundo);
    }
}
